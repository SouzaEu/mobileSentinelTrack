# Configuração para Backend Java - SentinelTrack Mobile
# Copie este conteúdo para .env quando o backend Java estiver pronto

# Backend Java - Desenvolvimento Local
EXPO_PUBLIC_USE_MOCKS=false
EXPO_PUBLIC_API_BASE_URL=http://localhost:8080/api/v1
EXPO_PUBLIC_ENVIRONMENT=development

# Backend Java - Staging
# EXPO_PUBLIC_USE_MOCKS=false
# EXPO_PUBLIC_API_BASE_URL=https://staging-api.sentineltrack.com/api/v1
# EXPO_PUBLIC_ENVIRONMENT=staging

# Backend Java - Produção
# EXPO_PUBLIC_USE_MOCKS=false
# EXPO_PUBLIC_API_BASE_URL=https://api.sentineltrack.com/api/v1
# EXPO_PUBLIC_ENVIRONMENT=production

# Configurações opcionais
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug
